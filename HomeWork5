public class HomeWork5 {
    public static void main(String[] args) {
        int[][] array = new int[2][2];

        array[0][0] = 10;
        array[0][1] = 7;
        array[1][0] = 98;
        array[1][1] = 4444444;

        for (int i = 0; i < array.length; i++) {
            for (int j = 0; j < array[i].length; j++) {
                System.out.print(array[i][j] + "\t");
            }
            System.out.println();
        }
    }

}
